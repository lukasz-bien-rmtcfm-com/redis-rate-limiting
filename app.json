{
    "name": "Redis Rate Limiting",
    "description": "App for testing request rate limits in NodeJS, Express and Redis",
    "repository": "https://github.com/RemoteCraftsmen/redis-rate-limiting",
    "keywords": ["node", "express", "redis"],
    "env": {
        "REDIS_ENDPOINT_URI": {
            "description": "Redis server URI in format redis://host:port",
            "required": true
        },
        "REDIS_PASSWORD": {
            "description": "Redis password",
            "required": true
        },
        "WEB_CONCURRENCY": {
            "description": "Number of Node processes (on Heroku)",
            "required": false,
            "value": "1"
        }
    }
}
